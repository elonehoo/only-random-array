{
  "name": "@elonehoo/only-random-array",
  "version": "0.0.4",
  "description": "Get consecutively unique elements from an array",
  "author": "Elone Hoo <huchengyea@163.com>",
  "license": "MIT",
  "homepage": "https://github.com/elonehoo/only-random-array#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elonehoo/only-random-array"
  },
  "bugs": "https://github.com/elonehoo/only-random-array/issues",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "rimraf dist && unbuild",
    "prepublishOnly": "npm run build",
    "release": "bumpp package.json --commit --push --tag && pnpm -r publish --access public",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@elonehoo/only-random": "^0.0.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.8",
    "@elonehoo/rimraf": "0.0.1",
    "@types/node": "20.4.4",
    "@vitest/coverage-c8": "0.33.0",
    "bumpp": "9.1.1",
    "eslint": "8.45.0",
    "pnpm": "8.6.9",
    "typescript": "5.1.6",
    "unbuild": "^2.0.0",
    "vite": "4.4.6",
    "vitest": "0.33.0"
  }
}
